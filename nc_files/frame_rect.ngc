o<frame_rect> sub
	; x1, y1, x2, y2, zstart, zend, mode
	; modes:
	; 1: CCW inside  pocket
	; 2: CW  inside  pocket
	; 3: CCW outside pocket
	; 4: CW  outside pocket

	G10 L0 ; re-read tool data
	G90	  ; absolute x,y,z
	G91.1 ; incremental i,j,k
	M101  ; enable Z-axis (M102 disables)
	G17   ; select XY plane for arcs

	#<minx> = #1
	#<maxx> = #3
	o103 if [#3 GT #1]
		#<minx> = #3
		#<maxx> = #1
	o103 endif

	#<miny> = #2
	#<maxy> = #4
	o104 if [#4 GT #2]
		#<miny> = #4
		#<maxy> = #2
	o104 endif


	o102     if [#7 EQ 1] ; CCW inside
		; plunge in center
		G0 X[[#1+#3]/2] Y[[#2+#4]/2]
		G0 Z#5
		G1 Z#6

		G1 X[#<maxx>-#5410/2]
		G1 Y[#<maxy>-#5410/2]
		G1 X[#<minx>+#5410/2]
		G1 Y[#<miny>+#5410/2]
		G1 X[#<maxx>-#5410/2]
		G1 Y[#<maxy>-#5410/2]

		; go to center and pull out
		G0 X[[#1+#3]/2] Y[[#2+#4]/2]
		G0 Z#5

	o102 elseif [#7 EQ 2] ; CW inside
		; plunge in center
		G0 X[[#1+#3]/2] Y[[#2+#4]/2]
		G0 Z#5
		G1 Z#6

		G1 X[#<maxx>-#5410/2]
		G1 Y[#<miny>+#5410/2]
		G1 X[#<minx>-#5410/2]
		G1 Y[#<maxy>-#5410/2]
		G1 X[#<maxx>-#5410/2]
		G1 Y[#<miny>+#5410/2]

		; go to center and pull out
		G0 X[[#1+#3]/2] Y[[#2+#4]/2]
		G0 Z#5

	o102 elseif [#7 EQ 3]

	o102 elseif [#7 EQ 4]

	o102 elseif [#7 EQ 5]

	o102 elseif [#7 EQ 6]

	o102 endif

o<bore> endsub

M2