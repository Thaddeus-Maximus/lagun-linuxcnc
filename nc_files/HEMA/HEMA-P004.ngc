#<_z_clearance> = 0.4
#<_rampang>     = 10
;#<_stepover>    = 2

;#<_overlap> = 0

; +1: Conventional milling
; +2: Both-ways milling
; +4: Plunge entry
; +8: Outside


G20  ; G21 mm, G20 in
G10 L1 P1 Z0.0 R[.25/2] ; set tool
T1   ; set tool to T1
M06  ; manual toolchange
G54  ; absolute coordinates
F250  ; mm/min (300 mm/min = about 1 ft/min)

; x positions
#11 = 32.4
#12 = 92.4
#13 = 152.4
#14 = 212.4
#15 = 272.4

o<m6cb> sub
	o<pocket_circ> call [#1/25.4][#2/25.4] [0.40] [+.02][-.24] [0][+4]
	o<pocket_circ> call [#1/25.4][#2/25.4] [0.24] [-.20][-.53] [0][+4]	
o<m6cb> endsub

o<m5cb> sub
	o<pocket_circ> call [#1/25.4][#2/25.4] [0.35] [+.02][-.20] [0][+4]
	o<pocket_circ> call [#1/25.4][#2/25.4] [0.20] [-.19][-.53] [0][+4]	
o<m5cb> endsub

o101 sub ; an array
	o<pocket_circ> call [#1][-10]        [ 9.0] [0.5][-5.0] [0][+4]
	o<pocket_circ> call [#1][-10] [5.2]       [0.5][-14] [0][+4]

	o<pocket_circ> call [#1 - 14.0][-26] [10.7] [0.5][-6.0] [0][+4]
	o<pocket_circ> call [#1 - 14.0][-26] [6.4] [0.5][-14] [0][+4]

	o<pocket_circ> call [#1 + 14.0][-26] [10.7] [0.5][-6.0] [0][+4]
	o<pocket_circ> call [#1 + 14.0][-26] [6.4] [0.5][-14] [0][+4]

	o<pocket_circ> call [#1 + 14.0][-54] [10.7] [0.5][-6.0] [0][+4]
	o<pocket_circ> call [#1 + 14.0][-54] [6.4] [0.5][-14] [0][+4]

	o<pocket_circ> call [#1 - 14.0][-54] [10.7] [0.5][-6.0] [0][+4]
	o<pocket_circ> call [#1 - 14.0][-54] [6.4] [0.5][-14] [0][+4]

o101 endsub

o103 sub
	o<pocket_circ> call [#1][-68+40] [17.0] [0.5][-2.2] [0.1] [+4]
	o<pocket_circ> call [#1][-68+40] [10.0] [-2.0][-14] [0.1] [+4]
o103 endsub

o10 if [#2 EQ 1]


	o101 call [#11]
	o101 call [#12]
	o101 call [#13]
	o101 call [#14]
	o101 call [#15]

	#<tr> = [#5410/2]
	#<ang> = 30
	#<z>   = 0.2
	#<endx> = 304.8

	#<y> = [-70]
	G0 X[-#<tr>-#<z>/TAN[#<ang>]*0.2] Y[#<y>]
	G0 Z[-#<z>*1.2]

	o103 while [#<y> LT 0]
		G1 X[-#<tr>+#<z>/TAN[#<ang>]*1.2] Z[#<z>*0.2]
		G0 Y[#<y>]
		#<y> = [#<y>+2]

		G1 X[-#<tr>-#<z>/TAN[#<ang>]*0.2] Z[-#<z>*1.2]
		G0 Y[#<y>]
		#<y> = [#<y>+2]

	o103 endwhile
	G0 Z[#<_z_clearance>]

	#<y> = [-70]
	G0 X[#<endx>+#<tr>+#<z>/TAN[#<ang>]*0.2] Y[#<y>]
	G0 Z[-#<z>*1.2]

	o104 while [#<y> LT 0]
		G1 X[#<endx>+#<tr>-#<z>/TAN[#<ang>]*1.2] Z[#<z>*0.2]
		G0 Y[#<y>]
		#<y> = [#<y>+2]

		G1 X[#<endx>+#<tr>+#<z>/TAN[#<ang>]*0.2] Z[-#<z>*1.2]
		G0 Y[#<y>]
		#<y> = [#<y>+2]

	o104 endwhile
	G0 Z[#<_z_clearance>]

o10 elseif [#2 EQ 2]
	o102 sub
		o<pocket_circle> call [#1][-70+40] [17.0] [0.5][-3.0] [0.1]
		o<pocket_circle> call [#1][-70+40] [10.0] [-3.0][-14] [0.1]
	o102 endsub

	o103 call [#11]
	o103 call [#12]
	o103 call [#13]
	o103 call [#14]
	o103 call [#15]
o10 endif

M2 ; end program
